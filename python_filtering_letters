#filtering with function and finding vowels letter

letters = ['u', 'a', 'q', 'c', 'i', 'd', 'z', 'p', 'e']

def check(letter):
  list_of_vowels = ['a', 'e', 'i', 'o', 'u']
 
if letter in list_of_vowels:
      return True
  else:
      return False
      
filtered_object = list(filter(check, letters))

print("The list of vowels is: ", filtered_object)
